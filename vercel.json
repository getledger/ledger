{
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build:webapp",
  "outputDirectory": "packages/webapp/build",
  "rewrites": [{
      "source": "/api/:slug*",
      "destination": "https://dev.ledger.ly/api/:slug*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}